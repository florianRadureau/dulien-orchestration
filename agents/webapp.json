{
  "mcpServers": {
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      }
    },
    "business-context": {
      "command": "node", 
      "args": ["../business-context-mcp/dist/index.js"]
    },
    "accessibility": {
      "command": "npx",
      "args": ["-y", "a11y-mcp"]
    }
  }
}
